    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-import-graphql": "^2.6.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.3",
    "cross-env": "^5.1.1",
    "cyclejs-test-helpers": "^2.0.0",
    "enzyme": "^3.2.0",
    "eslint": "^4.13.1",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-react": "^7.5.1",
    "fixme": "^0.4.4",
    "github-markdown-css": "^2.10.0",
    "html-looks-like": "^1.0.3",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "jsverify": "^0.8.3",
    "lint-staged": "^7.0.3",
    "node-while": "^1.0.1",
    "nodemon": "^1.12.7",
    "npm-run-all": "^4.1.2",
    "npm-watch": "^0.3.0",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-bundle-visualiser": "^1.0.2",
    "prettier": "^1.11.1",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "snabbdom-to-html": "^5.1.0",
    "testcafe": "^0.23.1",
    "testcafe-live": "^0.1.4",
    "testcafe-reporter-xunit": "^2.1.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.1.6",
    "wait-on": "^2.1.0"
  },
  "keywords": [
		"cyclejs",
		"arangodb",
		"parcel",
		"apollo",
		"graphql",
		"express"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "nyc": {
    "include": [
      "src/client/**/*.js",
      "src/server/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "test/**/*.spec.js",
      "client/reducers.js",
      "client/store.js",
      "client/routes.js",
      "server/util/setup-test-env.js",
      "server/util/test-helpers.js",
      "server/config.js",
      "server/dummyData.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
	"repository": "janat08/makpal",
  "license": "MIT"
}
