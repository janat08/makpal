{
	"name": "makpal.app",
  "version": "0.9.0",
	"description": "Cycle.js app with graphql/express/arangodb",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "deploy": "now",
    "start": "npm run prod",
    "dev": "npm-run-all --parallel dev:build dev:serve",
    "debug": "npm-run-all --parallel dev:build dev:debug",
    "prod": "npm run prod:build && npm run prod:serve",
    "clean": "rimraf dist ./node_modules/.cache/babel-loader",
    "cleangql": "rimraf node_modules/.cache/babel-loader",
    "dev:build": "npm run clean && cross-env NODE_ENV=development parcel watch src/client/index.js --out-dir dist/bundles",
    "dev:build:css": "npm run clean && cross-env NODE_ENV=development parcel watch src/client/index.scss --out-dir dist/bundles",
    "dev:serve": "yarn prod:copy-files && cross-env NODE_ENV=development node ./script/parcel-dev-server.js",
    "dev:debug": "cross-env NODE_ENV=development npx nodemon --exec babel-node --presets @babel/preset-env --extensions '.js,.jsx,.ts,.tsx' --delay 0.5 --watch src --watch test --inspect src/server/",
    "1dev:debug": "cross-env NODE_ENV=development DEBUG=express:* npx nodemon --exec babel-node --presets @babel/preset-env --extensions '.js,.jsx,.ts,.tsx' --delay 0.5 --watch src --watch test --inspect src/server/",
    "prod:build": "npm run clean && npm run prod:copy-files && npm run prod:build-server && npm run prod:build-client ",
    "prod:build-client": "cross-env NODE_ENV=production parcel build src/client/index.js --public-url / --out-dir dist/bundles --detailed-report",
    "prod:build-server": "cross-env NODE_ENV=production parcel build --target node ./src/server/index.ts --out-dir dist/server",
    "prod:copy-files": "cross-env npx copyfiles -u 1 'src/static/**/*' dist",
    "prod:serve": "cross-env NODE_ENV=production node ./dist/server/index.js",
    "test": "npm run lint && npm run test:unit",
    "e2e:dev": "testcafe-live -e -c 1 'chrome' test/e2e/ --src src",
    "test:unit:dev": "npx jest --config test/unit/jest.config.js --watch",
    "test:firefox:headless": "testcafe -e 'firefox:headless' test/e2e/",
    "test:chrome:headless": "testcafe -e 'chrome:headless' test/e2e/",
    "test:unit": "npx jest --config test/unit/jest.config.js",
    "lint": "npx eslint --ext jsx,js src test --fix",
    "todo": "npx fixme -i 'node_modules/**' -i '.git/**' -i 'dist/**' --skip note",
    "format": "prettier --config ./.prettierrc.json --write \"src/**/*.@(js|jsx|ts|tsx)\"",
		"codemod:Chai:ApolloUniversal": "jest-codemods"

  },
  "watch": {
    "test:quick": {
      "patterns": [
        "src",
        "test"
      ],
			"extensions": "js,jsx,ts,tsx",
      "quiet": false
    }
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "dependencies": {
    "@cycle/dom": "^20.2.0",
    "@cycle/history": "^6.10.0",
    "@cycle/http": "^14.9.0",
    "@cycle/isolate": "^3.2.0",
    "@cycle/run": "^4.1.0",
    "@cycle/state": "^1.0.0",
    "@cycle/storage": "^5.0.3",
    "@cycle/time": "^0.13.0",
    		"@types/parcel-env": "^0.0.0",
		"apollo-cache": "^1.1.21",
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-cache-router": "^1.1.6",
    "apollo-client": "^2.3.8",
    "apollo-link": "^1.2.3",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-schema": "^1.1.1",
    "apollo-link-state": "^0.4.1",
    "apollo-link-ws": "^1.0.8",
    "apollo-logger": "^0.3.0",
    "apollo-server-express": "^2.0.5",
    "apollo-utilities": "^1.0.20",
    "arangojs": "^6.4.0",
    "bcryptjs": "^2.4.3",
    "caller-callsite": "^2.0.0",
    "common": "^0.2.5",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cycle-storageify": "^4.0.1",
    "cyclejs-cookie": "^0.7.4",
    "cyclejs-utils": "^3.1.0",
    "cyclic-router": "^5.1.7",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-anywhere": "^3.1.0",
    "graphql-auth": "0.2.2",
    "graphql-fragments": "^0.1.0",
    "graphql-iso-date": "^3.3.0",
    "graphql-resolve-batch": "^1.0.2",
		"graphql-tools": "^4.0.3",
		"graphql-subscriptions": "^1.0.0",
    "graphql-tag": "^2.10.0",
    "immutability-helper": "^2.7.1",
    		"iterall": "^1.2.2",

    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "minilog": "^3.1.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "persistgraphql": "^0.3.10",
    "ramda": "^0.25.0",
    "react": "16.4.1",
    "react-apollo": "^2.1.11",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "serve-favicon": "^2.5.0",
    "snabbdom-pragma": "^2.7.0",
    "subscriptions-transport-ws": "^0.9.14",
    "switch-path": "^1.2.0",
    "universal-cookie": "^3.0.4",
    "universal-cookie-express": "^3.0.4",
    "winston": "^2.4.3",
    "xstream": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-transform-typescript": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
